<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="previewVM" type="com.application.app.modules.preview.data.viewmodel.PreviewVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearGroup79"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearRectangle3"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegreen_500">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup71"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_199pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFlechacorrecta"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_flechacorrecta"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtResumen"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.77"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{previewVM.previewModel.txtResumen}"
                        style="@style/txtRobotoromanmedium22" tools:text="@string/lbl_resumen"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearGroup12"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtResumen1"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_105pxh"
                    android:layout_marginStart="@dimen/_105pxh"
                    android:layout_marginTop="@dimen/_45pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{previewVM.previewModel.txtResumen1}"
                    style="@style/txtRobotoromanmedium24" tools:text="@string/lbl_resumen"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup73"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCalorias1"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_width="@dimen/_64pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_calorias1}" tools:src="@drawable/img_calorias1"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearGroup72"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_126pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.63"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPesoObjetivo"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{previewVM.previewModel.txtPesoObjetivo}"
                            style="@style/txtRobotoromanregular18" tools:text="@string/lbl_peso_objetivo"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt600Kg"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{previewVM.previewModel.txt600Kg}"
                            style="@style/txtRobotoromanregular14" tools:text="@string/lbl_60_0_kg"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup75"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCalorias2"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_64pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_calorias1_1}" tools:src="@drawable/img_calorias1_1"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearGroup74"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_weight="0.75"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:id="@+id/txtCalorasParaM"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:text="@{previewVM.previewModel.txtCalorasParaM}"
                            style="@style/txtRobotoromanregular18" tools:text="@string/msg_calor_as_para_m"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt2339Kcal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{previewVM.previewModel.txt2339Kcal}"
                            style="@style/txtRobotoromanregular14" tools:text="@string/lbl_2339_kcal"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup77"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCalorias3"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_marginStart="@dimen/_44pxh"
                        android:layout_width="@dimen/_64pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_calorias1_2}" tools:src="@drawable/img_calorias1_2"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearGroup76"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_98pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.68"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCalorasObjeti"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{previewVM.previewModel.txtCalorasObjeti}"
                            style="@style/txtRobotoromanregular18" tools:text="@string/msg_calor_as_objeti"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt15781929Kca"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{previewVM.previewModel.txt15781929Kca}"
                            style="@style/txtRobotoromanregular14" tools:text="@string/msg_1578_1929_kca"/>
                    </LinearLayout>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnCrearPlan"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_105pxh"
                    android:layout_marginStart="@dimen/_105pxh"
                    android:layout_marginTop="@dimen/_90pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_crear_plan"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_crear_plan"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
