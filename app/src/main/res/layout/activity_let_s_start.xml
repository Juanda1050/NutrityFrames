<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="letSStartVM" type="com.application.app.modules.letsstart.data.viewmodel.LetSStartVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearLetsstart"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearRectangle3"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegreen_500">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup81"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_132pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFlechacorrecta"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_flechacorrecta"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCreaUnaCuenta"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{letSStartVM.letSStartModel.txtCreaUnaCuenta}"
                        style="@style/txtRobotoromanmedium22" tools:text="@string/lbl_crea_una_cuenta"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearGroup5"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <TextView android:ellipsize="end"
                android:id="@+id/txtRegstrate"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_70pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{letSStartVM.letSStartModel.txtRegstrate}"
                style="@style/txtRobotoromanmedium24" tools:text="@string/lbl_reg_strate"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_100_radius_10"
                android:drawablePadding="@dimen/_10pxh"
                android:drawableStart="@drawable/img_perdidadepeso_1_1"
                android:ellipsize="end"
                android:hint="@string/msg_continuar_con_g"
                android:id="@+id/etContinuarConG"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_50pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={letSStartVM.letSStartModel.etContinuarConGValue}"
                android:textColorHint="@color/black_900" style="@style/etSolidRounded"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_100_radius_10"
                android:drawablePadding="@dimen/_10pxh"
                android:drawableStart="@drawable/img_perdidadepeso_1_2"
                android:ellipsize="end"
                android:hint="@string/msg_continuar_con_c"
                android:id="@+id/etContinuarConC"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={letSStartVM.letSStartModel.etContinuarConCValue}"
                android:textColorHint="@color/black_900" style="@style/etSolidRounded"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNutrity2022"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_25pxv"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_227pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{letSStartVM.letSStartModel.txtNutrity2022}"
                style="@style/txtRobotoromanregular14" tools:text="@string/lbl_nutrity_2022"/>
        </LinearLayout>
    </LinearLayout>
</layout>
