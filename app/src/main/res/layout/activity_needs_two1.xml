<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="needsTwo1VM" type="com.application.app.modules.needstwo1.data.viewmodel.NeedsTwo1VM"/>
    </data>
    <LinearLayout android:id="@+id/linearGroup184"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearRectangle3"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegreen_500">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup45"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_216pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFlechacorrecta"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_flechacorrecta"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSobreTi"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.73"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{needsTwo1VM.needsTwo1Model.txtSobreTi}"
                        style="@style/txtRobotoromanmedium22" tools:text="@string/lbl_sobre_ti"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearGroup53"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearTheComponent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_55pxh"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_300cornerRadius">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearProgressIndica"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_120pxh"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylegreen_500">
                    <View android:id="@+id/viewHackyLine"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginEnd="@dimen/_129pxh" android:layout_width="@dimen/_1pxh"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearHombres"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_55pxh"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStyle_1">
                <com.google.android.material.tabs.TabLayout
                    android:gravity="start"
                    android:id="@+id/tabLayoutGroup5"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    app:tabBackground="@drawable/selector_bg_gray_100_radius_15"
                    app:tabIndicatorColor="@android:color/transparent"
                    app:tabIndicatorHeight="0dp" app:tabMode="fixed"
                    app:tabSelectedTextColor="@color/black_900" app:tabTextColor="@color/white_A700"/>
            </LinearLayout>
            <androidx.viewpager2.widget.ViewPager2
                android:gravity="start"
                android:id="@+id/viewPagerGroup56"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_weight="1" android:layout_width="match_parent"/>
        </LinearLayout>
    </LinearLayout>
</layout>
