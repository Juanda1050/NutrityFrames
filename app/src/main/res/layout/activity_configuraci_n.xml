                        android:text="@{configuraciNVM.configuraciNModel.txtPerfil}"
                        style="@style/txtRobotoromanmedium22" tools:text="@string/lbl_perfil"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearGroup13"
