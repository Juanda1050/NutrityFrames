                    android:id="@+id/txtPesoObjetivo"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.69"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{configuraciNVM.configuraciNModel.txtPesoObjetivo}"
                    style="@style/txtRobotoromanmedium14_1" tools:text="@string/lbl_peso_objetivo"/>
