                    android:layout_marginStart="@dimen/_106pxh"
                    android:layout_weight="0.62"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{configuraciNVM.configuraciNModel.txtHombre}"
                    style="@style/txtRobotoromanregular14_1" tools:text="@string/lbl_hombre"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearInfo7"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtObjetivo"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.38"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{configuraciNVM.configuraciNModel.txtObjetivo}"
                    style="@style/txtRobotoromanmedium14_1" tools:text="@string/lbl_objetivo"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBajarDePeso"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_85pxh"
                    android:layout_weight="0.62"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{configuraciNVM.configuraciNModel.txtBajarDePeso}"
                    style="@style/txtRobotoromanregular14_1" tools:text="@string/lbl_bajar_de_peso"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
